<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:ImageSorter.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ImageSorter.Views.ProjectSelectionView"
			 x:DataType="vm:ProjectSelectionViewModel">


	<Grid>
		<StackPanel>
			<Button Content="SelectProject" Command="{ Binding SelectProject }"/>
			<Button Content="Go to main" Command="{ Binding GoNext }"/>
		</StackPanel>
	</Grid>
</UserControl>
